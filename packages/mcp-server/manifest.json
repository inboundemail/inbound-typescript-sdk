{
  "dxt_version": "0.2",
  "name": "inbound-docs-mcp",
  "version": "0.19.0",
  "description": "The official MCP Server for the Inbound API",
  "author": {
    "name": "Inbound",
    "email": "sdk@inbound.new"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inboundemail/inbound-typescript-sdk.git"
  },
  "homepage": "https://github.com/inboundemail/inbound-typescript-sdk/tree/main/packages/mcp-server#readme",
  "documentation": "https://inbound.new/docs",
  "server": {
    "type": "node",
    "entry_point": "index.js",
    "mcp_config": {
      "command": "node",
      "args": ["${__dirname}/index.js"],
      "env": {
        "INBOUND_API_KEY": "${user_config.INBOUND_API_KEY}"
      }
    }
  },
  "user_config": {
    "INBOUND_API_KEY": {
      "title": "api_key",
      "description": "Your Inbound API key. Include it in the Authorization header as: Bearer <your-api-key>",
      "required": true,
      "type": "string"
    }
  },
  "tools": [],
  "tools_generated": true,
  "compatibility": {
    "runtimes": {
      "node": ">=18.0.0"
    }
  },
  "keywords": ["api"]
}
